{
  "name": "open-shift-box",
  "version": "0.0.1",
  "description": "HtML / CSS exercice",
  "main": "index.js",
  "repository": "https://github.com/twbs/bootstrap",
  "watch": {
    "compile": {
      "patterns": ["client/less"],
      "extensions": "less"
    }
  },
  "scripts": {
    "compile": "rm -rf public/css/style.css && lessc client/less/style.less public/css/style.css",
    "watch": "npm-watch",
    "serve": "live-server",
    "start": "npm run serve & npm run watch"
  },
  "author": "Corinne Krych",
  "license": "Apache-2.0",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "jquery": "^3.1.1"
  },
  "devDependencies": {
    "less": "^2.7.2",
    "live-server": "^1.2.0",
    "npm-watch": "^0.1.8"
  }
}
